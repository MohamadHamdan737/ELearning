@model ELearning.Bl.Models.AboutModel

@{
    ViewData["Title"] = "Delete";
    Layout = "_AdminLayout";

}
<link href="~/css/style2.css" rel="stylesheet" />
<div class="ml">
<h4>Are you sure you want delete this?</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Delete" method="post">
            <div asp-validation-summary="All" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="MainTitale"  class="control-label"></label>
                <input asp-for="MainTitale" readonly class="form-control" />
                <span asp-validation-for="MainTitale" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Subject" class="control-label"></label>
                <textarea rows="4" asp-for="Subject" readonly class="form-control" ></textarea>
                <span asp-validation-for="Subject" class="text-danger"></span>
            </div>
            <div class="form-group">
                    <input type="submit" value="Delete" class="btn btn-lg btn-outline-danger" />
                    <a asp-action="About" asp-controller="Admin" class="btn btn-lg btn-default">Cancel</a>

            </div>
        </form>
    </div>
</div>
</div>



